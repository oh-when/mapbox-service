{
  "compilerOptions": {
    "esModuleInterop": true,
    "importsNotUsedAsValues": "preserve",
    "sourceMap": true,
    "allowJs": true,
    "module": "commonjs",
    "target": "es5",
    "skipLibCheck": true,
    "declaration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "lib": [
      "es2015",
      "dom",
      "es2017"
    ]
  }
}
